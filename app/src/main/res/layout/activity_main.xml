<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FBFAF1"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/main_cus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/round1"
        android:fontFamily="sans-serif-condensed"
        android:padding="10dp"
        android:text="@string/cus"
        android:textColor="#FDFBF1"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/main_res"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/round1"
        android:fontFamily="sans-serif-condensed"
        android:padding="10dp"
        android:text="@string/res"
        android:textColor="#FDFBF1"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <ImageView
        android:id="@+id/main_img1"
        android:layout_width="375dp"
        android:layout_height="385dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon" />

    <ImageView
        android:id="@+id/main_img2"
        android:layout_width="158dp"
        android:layout_height="179dp"
        app:layout_constraintBottom_toBottomOf="@+id/main_img1"
        app:layout_constraintEnd_toEndOf="@+id/main_img1"
        app:layout_constraintStart_toStartOf="@+id/main_img1"
        app:layout_constraintTop_toTopOf="@+id/main_img1"
        app:srcCompat="@drawable/in" />


</androidx.constraintlayout.widget.ConstraintLayout>